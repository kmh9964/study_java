package com.hanul.variable;

public class Ex02_number {

	public static void main(String[] args) {

		
		byte no1 = -128;
		int no2 = 128;
		
		
		
		System.out.println(no1 + no2);
		
		float no3 = 0.1;
		float no4 = 0.3;
		System.out.println(no1 + no2);
		System.out.println(no3 + no4);
		
		
		
	}

}
