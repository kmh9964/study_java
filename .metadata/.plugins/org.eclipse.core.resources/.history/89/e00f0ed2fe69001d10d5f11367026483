package com.hanul.control;

public class Ex08_if02 {

	public static void main(String[] args) {

	//if
		int score = 83;
		if(score >= 90) {
			System.out.println(score + "90");
		}else if(score >= 80) {
			System.out.println(score + "80");
		}else if(score >= 70) {
			System.out.println(score + "70");
	    }	
		
		
		
	
		
		
		
		
		
		
		
		
	}

}
