package com.hanul.control;

public class Ex19_Dowhile {

	public static void main(String[] args) {

		//do{~} ~ while(조건식);문은 무조건 한번은 실행한다.while(조건식)뒤에 무조건 ;이 와야 된다
		
		//1~10까지 출력해보자
		int no1 = 1; //1~10까지 변하는 변수이다
		do {
			System.out.println(no1);
			no1++;
		}while(no1 <= 10);
		
		
		
		
		
		
		
		
		
	}

}
