package com.hanul.variable;

public class Ex02_number {

	public static void main(String[] args) {

		
		byte no1 = -128;
		int no2 = 128;
		
		
		
		System.out.println(no1 + no2);
		
		float no3 = 0.1f; //실수형 데이터 타입 중 float는 숫자뒤에 f을 붙여줘야 된다
		float no4 = 0.3f;
		System.out.println(no1 + no2);
		System.out.println(no3 + no4);
		
		
		
	}
	
public static void main(String[] args) {

		
			
		float no1 = 0.1f; //실수형 데이터 타입 중 float는 숫자뒤에 f을 붙여줘야 된다
		float no2 = 0.3f;
		System.out.println(no1 + no2);
		System.out.println(no3 + no4);

}
